{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".ts"]
        },
        "typescript": {}
      }
    },
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    }  
  }
  